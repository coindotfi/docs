              "arpc/ibc-bridge",
              "arpc/contract-addresses",
              "arpc/core-concepts",
              "arpc/beacon-layer",
              "arpc/application-chain-layer",
              "arpc/qotp-protocol",
              "arpc/quantum-one-time-aamm",
              "arpc/security-model",
              "arpc/zkm-trustless-universal-settlement",
              "arpc/testing",
              "arpc/local-development"
